            <string>/// Control the menu

// Get Input
get_input();

if (alarm[0] &lt;= 0) { 
    if (down || yaxis &gt; .5) {
        if (menu_index &lt; array_length_1d(option)-1) {
            menu_index++;
        } else {
            menu_index = 0;
        }
        alarm[0] = room_speed/6;
    }
    
        if (up) {
            if (menu_index &gt; 0) {
                menu_index--;
            } else {
                menu_index = array_length_1d(option)-1;
            }      
            alarm[0] = room_speed/6;
    }       
    
    if (jump) { 
        switch (menu_index) {
            case 0:
            {
            room_goto(r_zero);
                break;
            }
            
            case 1:
            {
            load_game();
                break;
